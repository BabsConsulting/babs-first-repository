<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Donate Application</display-name>
    <!--  Log4J JMX initialization 
    <listener>
        <listener-class>
            gov.grantsolutions.util.logging.LoggingConfiguration
        </listener-class>
    </listener> -->
    <servlet>
        <servlet-name>DonateServlet</servlet-name>
        <servlet-class>net.mscchoir.donate.web.DonateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DonateServlet</servlet-name>
        <url-pattern>/donate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
